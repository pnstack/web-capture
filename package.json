{
  "name": "template-nodejs-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json",
    "clean": "rimraf dist/",
    "dev": "ts-node ./src/index.ts",
    "backup": "ts-node ./src/backup.ts",
    "checkdb": "ts-node ./src/checkdb.ts",
    "dev:watch": "nodemon --quiet",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node ./dist/cjs/index.js",
    "start:dev": "yarn build  && node ./dist/cjs/index.js",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest",
    "prisma:watch": "prisma generate --schema prisma/old.prisma --watch & prisma generate --schema prisma/dev.prisma --watch",
    "prisma": "prisma generate --schema prisma/old.prisma & prisma generate --schema prisma/dev.prisma",
    "prisma:dev:pull": "prisma db pull --schema prisma/dev.prisma",
    "prisma:dev:push": "prisma db push --schema prisma/dev.prisma",
    "prisma:dev:migrate": "prisma migrate dev --schema prisma/dev.prisma",
    "crawler": "ts-node ./src/crawler.ts",
    "crawler:db": "ts-node ./src/crawler-child.ts"
  },
  "author": "npv2k1",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.47.2",
    "axios": "^1.7.9",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "qs": "^6.13.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.3.74",
    "@swc/helpers": "^0.5.1",
    "@types/express": "^5.0.0",
    "@types/jest": "28.1.6",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.16.19",
    "@types/qs": "^6.9.16",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "28.1.3",
    "nodemon": "^2.0.22",
    "prettier": "2.7.1",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typescript": "4.7.4",
    "unbuild": "^1.2.1"
  }
}
